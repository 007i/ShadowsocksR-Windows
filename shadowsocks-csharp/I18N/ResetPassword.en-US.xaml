<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="ResourceDictionaryName">en-US</sys:String>

    <sys:String x:Key="Title">ResetPassword</sys:String>

    <sys:String x:Key="OldPassword">Old password</sys:String>
    <sys:String x:Key="NewPassword">New password</sys:String>
    <sys:String x:Key="ConfirmPassword">Confirm new password</sys:String>
    <sys:String x:Key="Ok">OK</sys:String>

    <sys:String x:Key="PasswordMatchError">Password NOT match</sys:String>

</ResourceDictionary>